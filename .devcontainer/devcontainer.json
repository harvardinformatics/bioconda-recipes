{
  "image": "quay.io/bioconda/bioconda-utils-build-env-cos7:latest",
  "postCreateCommand": "echo 'conda activate' > /etc/profile.d/z-conda-activate.sh && chmod +x /etc/profile.d/z-conda-activate.sh",
  "customizations": {
    "vscode": {
      "settings": {
        "git.path": "/opt/conda/bin/git"
      }
    }
  }
}